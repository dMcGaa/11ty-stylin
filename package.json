{
    "name": "11ty-stylin",
    "version": "0.0.1",
    "description": "11ty-stylin - A pracctice style template built with Eleventy",
    "scripts": {
        "build": "npm run gulp-build && eleventy --config=eleventy.config.js",
        "dev": "concurrently \"npm:gulp-watch\" \"npm:eleventy\"",
        "eleventy": "ELEVENTY_ENV=development eleventy --config=eleventy.config.js --serve",
        "gulp-watch": "gulp watch",
        "gulp-build": "gulp build",
        "start": "npm run dev"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dmcgaa/11ty-stylin.git"
    },
    "author": "David McGaa",
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy": "^0.7.1",
        "concurrently": "^5.0.0",
        "gulp": "^3.9.1",
        "gulp-sass": "^4.0.2"
    }
}
