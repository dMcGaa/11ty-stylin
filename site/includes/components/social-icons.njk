<ul class="">

    {% for icon, link in site.social_links %}

    <li class="">

        <a href="{{ link | safe }}" class="" aria-label="{{ icon | capitalize }}">

            <svg class="" fill="currentColor" role="img" aria-hidden="true" width="18" height="18">
                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/images/icons/icon-library.svg#icon-{{ icon }}"></use>
            </svg>

        </a>

    </li>

    {% endfor %}

</ul>
